<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or metadata.title }}</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css2?family=PT+Sans+Narrow:wght@400;700&family=PT+Sans:ital,wght@0,400;0,700;1,400&display=swap" rel="stylesheet">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path | url }}" type="application/json" title="{{ metadata.title }}">
    
    {% set css %}
    {% include "../../css/prism.base16.monokai.dark.css" %}
    {% include "../../css/base.css" %}
    {% include "../../css/home.css" %}
    {% include "../../css/footer.css" %}
    {% include "../../css/post.css" %}
    {% include "../../css/postview.css" %}
    {% endset %}
    <style>
      {{css | cssmin | safe}}
    </style>
    {% seo "" %}

  </head>
  <body>
    <section class="container-fluid align-items-center">
      <div class="row dark-bg">
        {% include "layouts/header.njk" %}
      </div>
      <div class="row main-row">
        <div class="col-lg-12">
          <div class="row">
            <div class="col-lg-12">
              {{ content | safe }}
            </div>
          </div>
        </div>
      </div>
    {% set links = links %}
    {% include "layouts/footer.njk" %}
    </section>

    <script defer src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script defer src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>
    <script defer src="https://kit.fontawesome.com/0a5d5a6a83.js" crossorigin="anonymous"></script>
    <script>
    let themes = JSON.parse('{{ themes | dump }}'); 
    function dark() {
      for(x in themes["dark"]) {
        document.documentElement.style.setProperty(x, themes["dark"][x]);
      }
    }
    function light() {
      for(x in themes["light"]) {
        document.documentElement.style.setProperty(x, themes["light"][x]);
      }
    }
    let now = new Date();
    let eveningTime = new Date(now.getFullYear(), now.getMonth(), now.getDate(), 21,27,0,0) - now;
    if(eveningTime < 0) {
      dark()
    } else {
      setTimeout(() => dark(), eveningTime);
    }
    </script>
  </body>
</html>